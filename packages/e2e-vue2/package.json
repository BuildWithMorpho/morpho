{
  "name": "@builder.io/e2e-vue2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "yarn run morpho && tsc --noEmit && vite build",
    "dev": "yarn run morpho && tsc --noEmit && vite",
    "e2e": "../../node_modules/.bin/playwright test",
    "morpho": "syncdir ../e2e-app/src src --deleteOrphaned --quiet && morpho build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@builder.io/e2e-app": "workspace:*",
    "@builder.io/morpho": "workspace:*",
    "@builder.io/morpho-cli": "workspace:*",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "sync-directory": "^5.1.5",
    "typescript": "^4",
    "vite": "^2.9.10",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-vue2": "^1.2.1",
    "vue-template-compiler": "^2.6.12"
  }
}
