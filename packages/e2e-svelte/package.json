{
  "name": "@builder.io/e2e-svelte",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "yarn run morpho && yarn run check && vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "yarn run morpho && vite",
    "e2e": "../../node_modules/.bin/playwright test",
    "morpho": "syncdir ../e2e-app/src src --deleteOrphaned --quiet && morpho build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@builder.io/e2e-app": "workspace:*",
    "@builder.io/morpho": "workspace:*",
    "@builder.io/morpho-cli": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.7",
    "sync-directory": "^5.1.5",
    "tslib": "^2.4.0",
    "typescript": "^4",
    "vite": "^2.9.9"
  }
}
